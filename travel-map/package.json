  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-simple-maps": "^3.0.6",
    "@types/react-transition-group": "^4.4.12",
