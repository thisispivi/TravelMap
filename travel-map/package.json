    "react-i18next": "^15.4.0",
    "react-image-gallery": "^1.3.0",
    "react-photo-album": "^3.0.2",
    "react-router-dom": "^7.1.3",
    "react-simple-maps": "^3.0.0",
    "react-tooltip": "^5.28.0",
    "remeda": "^2.20.0",
    "sass": "^1.83.4"
