    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-qrcode": "^0.2.4",
    "vite-plugin-svgr": "^4.3.0"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.{js,css,md,html,json}": "npm run format"
  },
  "browserslist": {
    "production": [
      ">0.01%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.01%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
